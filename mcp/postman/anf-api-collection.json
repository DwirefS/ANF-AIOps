  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Generate request ID for tracking",
          "pm.globals.set('requestId', pm.variables.replaceIn('{{$guid}}'));",
          "",
          "// Set current timestamp",
          "pm.globals.set('timestamp', new Date().toISOString());",
          "",
          "// Set API version",
          "pm.globals.set('apiVersion', '2025-03-01');"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Global test for response validation",
          "pm.test('Response status should be successful', function () {",
          "    pm.expect(pm.response.code).to.be.oneOf([200, 201, 202, 204]);",
          "});",
          "",
          "pm.test('Response should have proper headers', function () {",
          "    pm.expect(pm.response.headers.get('Content-Type')).to.include('application/json');",
          "});",
          "",
          "pm.test('Response time should be reasonable', function () {",
          "    pm.expect(pm.response.responseTime).to.be.below(10000);",
          "});",
          "",
          "// Store response data for subsequent requests",
          "if (pm.response.code === 200 || pm.response.code === 201) {",
          "    try {",
          "        const responseData = pm.response.json();",
          "        if (responseData.data && responseData.data.id) {",
          "            pm.globals.set('lastResourceId', responseData.data.id);",
          "        }",
          "        if (responseData.data && responseData.data.name) {",
          "            pm.globals.set('lastResourceName', responseData.data.name);",
          "        }",
          "    } catch (e) {",
          "        console.log('Unable to parse response JSON');",
          "    }",
          "}"
        ]
      }
    }
  ],
